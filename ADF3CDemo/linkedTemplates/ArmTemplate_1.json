									},
									"directory": {
										"value": "@pipeline().parameters.DirectoryName",
										"type": "Expression"
									},
									"file": {
										"value": "@pipeline().parameters.FileName",
										"type": "Expression"
									}
								}
							}
						],
						"outputs": [
							{
								"referenceName": "SO_Synapse_Dataset",
