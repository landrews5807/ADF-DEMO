		{
			"name": "[concat(parameters('factoryName'), '/Step 2 - import from file to staging table')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy Data - from blob storage to staging table",
